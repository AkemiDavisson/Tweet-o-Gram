<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=301880
  -->
<configuration>
  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--twitter keys-->
    <add key="oAuthUrl" value="https://api.twitter.com/oauth2/token"/>
    <add key="oAuthConsumerKey" value="#YOURCONSUMERKEYHERE#"/>
    <add key="oAuthConsumerSecret" value="#YOURCONSUMERSECRET#"/>
    <add key="twitterAccessToken" value="#YOURTWITTERACCESSTOKENHERE#"/>
    <add key="tokenType" value="bearer"/>
    <add key="searchFormat" value="https://api.twitter.com/1.1/search/tweets.json?q=%23{0}&amp;result_type=mixed&amp;count=12"/>
    <add key="resendSearchFormat" value="https://api.twitter.com/1.1/search/tweets.json?q=%23{0}&amp;result_type=mixed&amp;since_id={1}&amp;count=12"/>
    <add key="getMoreFormat" value="https://api.twitter.com/1.1/search/tweets.json?q=%23{0}&amp;result_type=mixed&amp;max_id={1}&amp;count=12"/>
    <add key="trendingFormat" value="https://api.twitter.com/1.1/trends/place.json?id=23424977"/>
    <!--instagram keys-->
    <add key="oAuthUrlGram" value="https://instagram.com/oauth/authorize/?client_id={0}&amp;redirect_uri={1}&amp;response_type=token"/>
    <add key="accessToken" value="#YOURACCESSTOKENHERE#"/>
    <add key="clientId" value="#YOURCLIENTIDHERE" />
    <add key="clientSecret" value="#YOURCLIENTSECRET#" />
    <add key="instagramSearchFormat" value="https://api.instagram.com/v1/tags/{0}/media/recent?access_token={1}&amp;count=12"/>
    <add key="instagramResendSearchFormat" value="https://api.instagram.com/v1/tags/{0}/media/recent?access_token={1}&amp;min_tag_id={2}&amp;count=12"/>
    <add key="instagramGetMoreFormat" value="https://api.instagram.com/v1/tags/{0}/media/recent?access_token={1}&amp;max_tag_id={2}&amp;count=12"/>
    <add key="instagramTrendingFormat" value="https://api.instagram.com/v1/media/popular?access_token={0}"/>

  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
</configuration>
